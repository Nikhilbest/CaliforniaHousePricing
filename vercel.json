{
    "version": 2,
    "builds": [
        {
          "src": "vercel_app/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "app.py"}
    ],
    "rewrites":[{
        "source" : "(/.*)",
        "destination" : "/"
    }]
}